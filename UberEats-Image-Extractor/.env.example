# Firecrawl API Configuration
FIRECRAWL_API_KEY=your-firecrawl-api-key-here
FIRECRAWL_API_URL=https://api.firecrawl.dev

# Cache Configuration
# Maximum age for cached content in seconds (default: 172800 = 2 days)
FIRECRAWL_CACHE_MAX_AGE=172800

# Server Configuration
PORT=3007
NODE_ENV=production

# Debug Mode
# Set to 'true' to enable detailed logging
DEBUG_MODE=false

# Supabase Configuration (Optional - for database persistence)
# Get these from your Supabase project settings
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key-here

# CloudWaitress Rate Limiting
# Minimum delay between account creations in milliseconds (default: 60000 = 60 seconds)
# Set to 0 to disable rate limiting
CLOUDWAITRESS_RATE_LIMIT_MS=60000